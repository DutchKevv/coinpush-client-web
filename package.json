{
  "name": "coinpush-client-web",
  "displayName": "App",
  "version": "0.0.26",
  "description": "CoinPush client Web",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchKevv/coinpush-client-web.git"
  },
  "main": "index.js",
  "scripts": {
    "_start": "ng serve",
    "start": "npm run _start -- --proxy-config ./src/proxy.conf.json",
    "build": "npm run prod",
    "prod": "ng build --prod --no-progress",
    "build-stats": "ng build --build-optimizer --prod --aot --stats-json --output-hashing=none && npm run show-report",
    "build-verbose": "ng build --build-optimizer --prod --stats-json --output-hashing=none --verbose > build.txt",
    "load-symbol-images": "node ./tools/load-symbol-images.js",
    "build-spritesheet": "node ./tools/build-spritesheet.js",
    "show-report": "webpack-bundle-analyzer ../static/clients/web/stats.json",
    "d-build": "docker-compose build",
    "d-build-prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build --no-cache",
    "d-start": "docker-compose up",
    "d-prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "d-kill": "docker-compose kill",
    "d-rm": "docker-compose rm -f",
    "d-log": "docker-compose logs -f --tail=20",
    "k-delete-pod": "kubectl delete deployment cp-client-web-pod || true",
    "k-create-pod": "kubectl create -f deployment.yml"
  },
  "author": "HK Brandsma",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular-devkit/build-angular": "0.800.0-beta.7",
    "@angular/animations": "^8.0.0-beta.7",
    "@angular/cdk": "^7.3.2",
    "@angular/cli": "8.0.0-beta.7",
    "@angular/common": "8.0.0-beta.7",
    "@angular/compiler": "8.0.0-beta.7",
    "@angular/compiler-cli": "8.0.0-beta.7",
    "@angular/core": "8.0.0-beta.7",
    "@angular/forms": "8.0.0-beta.7",
    "@angular/material": "7.3.4",
    "@angular/platform-browser": "8.0.0-beta.7",
    "@angular/router": "8.0.0-beta.7",
    "bootstrap": "4.3.1",
    "@coinpush/shared": "file:../coinpush-shared",
    "deepmerge": "3.2.0",
    "highcharts": "7.0.3",
    "localforage": "1.7.3",
    "rxjs": "6.4.0",
    "socket.io-client": "2.2.0",
    "time-ago": "0.2.1",
    "typescript": "3.2.4",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "tslint": "5.14.0",
    "webpack-bundle-analyzer": "3.1.0"
  }
}
