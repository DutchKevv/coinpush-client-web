{
  "name": "coinpush",
  "displayName": "App",
  "version": "0.0.26",
  "description": "CoinPush client",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchKevv/CoinPush.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "ng serve --proxy-config ./src/proxy.conf.json",
    "build": "npm run prod",
    "prod": "ng build --prod --no-progress",
    "build-stats": "ng build --build-optimizer --prod --aot --stats-json --output-hashing=none && npm run show-report",
    "build-verbose": "ng build --build-optimizer --prod --stats-json --output-hashing=none --verbose > build.txt",
    "load-symbol-images": "node ./tools/load-symbol-images.js",
    "build-spritesheet": "node ./tools/build-spritesheet.js",
    "show-report": "webpack-bundle-analyzer ../static/clients/web/stats.json"
  },
  "author": "HK Brandsma",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular-devkit/build-angular": "0.14.0-beta.0",
    "@angular/animations": "^8.0.0-beta.3",
    "@angular/cdk": "^7.3.1",
    "@angular/cli": "8.0.0-beta.0",
    "@angular/common": "8.0.0-beta.3",
    "@angular/compiler": "8.0.0-beta.3",
    "@angular/compiler-cli": "8.0.0-beta.3",
    "@angular/core": "8.0.0-beta.3",
    "@angular/forms": "8.0.0-beta.3",
    "@angular/material": "7.3.1",
    "@angular/platform-browser": "8.0.0-beta.3",
    "@angular/router": "8.0.0-beta.3",
    "bootstrap": "4.3.0",
    "coinpush": "file:../shared/modules/coinpush",
    "deepmerge": "3.1.0",
    "highcharts": "7.0.3",
    "localforage": "1.7.3",
    "rxjs": "6.4.0",
    "socket.io-client": "2.2.0",
    "time-ago": "0.2.1",
    "typescript": "3.2.4",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "tslint": "5.12.1",
    "webpack-bundle-analyzer": "3.0.3"
  }
}
